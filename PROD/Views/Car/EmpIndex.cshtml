@model IEnumerable<PROD.Models.CustModel>

@{
    ViewBag.Title = "EmpIndex";
}

<h2>@Html.ActionLink("Car List", "Index") || EmployeeList</h2>


<table class="table table-bordered table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Customerid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoyaltyPoints)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Customerid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoyaltyPoints)
        </td>
        <td>
            @Html.ActionLink("OrderedRentals", "OrderedRentals", new {  id=item.Customerid },new {@class="btn btn-primary"}) 
         
        </td>
    </tr>
}

</table>
